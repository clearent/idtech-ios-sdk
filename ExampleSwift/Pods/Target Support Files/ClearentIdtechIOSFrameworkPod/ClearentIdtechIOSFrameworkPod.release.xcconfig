ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
ARCHS = arm64 x86_64
CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/ClearentIdtechIOSFrameworkPod
DEFINES_MODULE = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/ClearentIdtechIOSFrameworkPod/ClearentIdtechIOSFrameworkPod" "${PODS_XCFRAMEWORKS_BUILD_DIR}/ClearentIdtechIOSFrameworkPod"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
OTHER_LDFLAGS = $(inherited) -framework "AVFoundation" -framework "AudioToolbox" -framework "CFNetwork" -framework "ExternalAccessory" -framework "MediaPlayer"
OTHER_SWIFT_FLAGS = $(inherited) -Xcc -Wno-error=non-modular-include-in-framework-module
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/ClearentIdtechIOSFrameworkPod
PODS_XCFRAMEWORKS_BUILD_DIR = $(PODS_CONFIGURATION_BUILD_DIR)/XCFrameworkIntermediates
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
VALID_ARCHS = arm64
VALID_ARCHS[sdk=iphonesimulator*] = x86_64
